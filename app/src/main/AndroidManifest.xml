<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.johnosezele1.instagramclone">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.johnosezele1.instagramclone.Home.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.johnosezele1.instagramclone.Likes.LikesActivity"> </activity>
        <activity android:name="com.johnosezele1.instagramclone.Profile.ProfileActivity"> </activity>
        <activity android:name="com.johnosezele1.instagramclone.Search.SearchActivity"> </activity>
        <activity android:name="com.johnosezele1.instagramclone.Share.ShareActivity"> </activity>
        <activity android:name="com.johnosezele1.instagramclone.Profile.AccountSettingsActivity"> </activity>
        <activity android:name="com.johnosezele1.instagramclone.Login.RegisterActivity"></activity>
        <activity android:name="com.johnosezele1.instagramclone.Login.LoginActivity"></activity>
    </application>

</manifest>